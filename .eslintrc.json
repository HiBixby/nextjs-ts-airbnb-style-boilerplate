{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "next/core-web-vitals",
    "airbnb",
    "airbnb-typescript",
    "plugin:prettier/recommended"
  ],
  "plugins": ["prettier"],
  "parserOptions": { "project": "./tsconfig.json" },
  "rules": {
    "react/jsx-props-no-spreading": "off",
    "prettier/prettier": ["error", { "endOfLine": "auto" }]
  }
}
